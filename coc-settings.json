{
  "eslint.filetypes": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "typescript.tsx",
    "vue"
  ],
  "diagnostic.checkCurrentLine": true,
  "eslint.autoFixOnSave": true,
  "tsserver.enableJavascript": true,
  "list.source.files.args": [
    "--hidden",
    "--files"
  ],
  "list.maxHeight": 30,
  "list.source.grep.args": [
    "--smart-case"
  ]
}
